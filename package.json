    "@types/node": "14.14.34",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "babel-jest": "26.6.3",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.65.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.63.4",
    "react-native-webview": "11.2.3",
    "rimraf": "3.0.2",
