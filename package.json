{
  "name": "react-native-react-bridge",
  "version": "0.5.0",
  "description": "An easy way to integrate your React (or Preact) app into React Native app with WebView.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "tsc": "tsc -p . --noEmit",
    "tsc:d": "tsc -p . --outDir lib -d --emitDeclarationOnly",
    "prepublishOnly": "rimraf lib && npm run build && npm run tsc:d"
  },
  "devDependencies": {
    "@babel/core": "7.14.0",
    "@babel/preset-env": "7.14.0",
    "@babel/traverse": "7.14.0",
    "@rollup/plugin-typescript": "8.2.1",
    "@types/node": "15.0.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "babel-jest": "26.6.3",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.65.0",
    "preact": "10.5.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.63.4",
    "react-native-webview": "11.4.0",
    "rimraf": "3.0.2",
    "rollup": "2.46.0",
    "tslib": "2.2.0",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "react-native": ">= 0.60.0",
    "react-native-webview": ">= 9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inokawa/react-native-react-bridge.git"
  },
  "keywords": [
    "react-native",
    "react",
    "react-dom",
    "preact",
    "metro",
    "webview",
    "html",
    "react-hooks",
    "webassembly"
  ],
  "author": "inokawa <stratoooo-taster@yahoo.co.jp> (https://github.com/inokawa/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/inokawa/react-native-react-bridge/issues"
  },
  "homepage": "https://github.com/inokawa/react-native-react-bridge#readme"
}
